html
    head
        style
            include adminuser.css
    body
        h1(id="top") Welcome to Online Pharmacy
        div(id="line")
            p Hello, Admin
        div(id="main")
            h1 Users
            table
                tr
                    th User ID
                    th User Name
                    th Age
                    th
                - for(i=1;i<=id.length;i++)
                    tr
                        td #{id[i-1]}
                        td #{name[i-1]}
                        td #{age[i-1]}
                        td
                            a(href=`/deluser/${id[i-1]}`) Remove User
        div(id="bottom")
            a(href='/logout' class="btmbtns") Log Out
            a(href='/adminspage' class="btmbtns") Go Back